public class Excercise6 {
	
	@Test
	public void testExcercise6() {
		
		
		WebDrivder driver = new ChromeDriver();
		
		driver.get("https://the-internet.herokuapp.com/");
		
		driver.findElement(By.linkText("Dropdown")).click();
		
		driver.quit();
	}

}
